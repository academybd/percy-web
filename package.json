{
  "name": "percy-web",
  "version": "1.0.0",
  "description": "Percy web application",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "build": "ember build",
    "build-production": "yarn run clean && ember build --environment=production",
    "clean": "rm -rf dist/* tmp/*",
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "lint:fix": "./node_modules/eslint/bin/eslint.js . --fix",
    "precommit": "lint-staged",
    "start": "ember server",
    "start-production-for-testing": "ember server --environment=production",
    "test": "ember test",
    "test:node_debug": "node debug node_modules/ember-cli/bin/ember test --server",
    "test:parallel": "PERCY_PARALLEL_TOTAL=4 ember exam --split=4 --parallel",
    "test:server": "ember test --server"
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/eslint/bin/eslint.js --fix",
      "git add"
    ]
  },
  "repository": "percy/percy-web",
  "author": "Perceptual Inc.",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.6.0",
    "ember-ajax": "^3.0.0",
    "ember-changeset": "^1.2.3",
    "ember-changeset-validations": "^1.2.8",
    "ember-cli": "^2.16.1",
    "ember-cli-app-version": "^3.1.0",
    "ember-cli-autoprefixer": "^0.8.1",
    "ember-cli-babel": "^6.8.2",
    "ember-cli-chai": "^0.4.3",
    "ember-cli-dependency-checker": "^2.0.1",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-htmlbars": "^2.0.3",
    "ember-cli-htmlbars-inline-precompile": "^1.0.2",
    "ember-cli-inject-live-reload": "^1.7.0",
    "ember-cli-mirage": "0.3.3",
    "ember-cli-mocha": "^0.14.4",
    "ember-cli-moment-shim": "^3.4.0",
    "ember-cli-sass": "^7.0.0",
    "ember-cli-shims": "^1.1.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-concurrency": "^0.8.12",
    "ember-data": "2.11.3",
    "ember-data-factory-guy": "^2.13.13",
    "ember-exam": "^0.8.1",
    "ember-export-application-global": "^2.0.0",
    "ember-fakerjs": "^0.1.0",
    "ember-font-awesome": "^3.0.5",
    "ember-inflector": "^2.0.1",
    "ember-load-initializers": "^1.0.0",
    "ember-modal-dialog": "^2.3.0",
    "ember-moment": "^7.5.0",
    "ember-page-title": "^3.2.2",
    "ember-percy": "^1.2.19",
    "ember-power-select": "^1.9.6",
    "ember-remarkable": "^3.6.1",
    "ember-resolver": "^4.5.0",
    "ember-route-action-helper": "^2.0.6",
    "ember-simple-auth": "^1.4.0",
    "ember-source": "~2.16.0",
    "ember-tether": "^1.0.0-beta.0",
    "ember-truth-helpers": "^2.0.0",
    "ember-watson": "^0.9.1",
    "eslint": "^4.10.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-prettier": "^2.3.1",
    "glob": "^7.1.2",
    "http-proxy": "^1.16.2",
    "husky": "^0.14.3",
    "lint-staged": "^4.3.0",
    "loader.js": "^4.6.0",
    "morgan": "^1.9.0",
    "percy-docs": "git+https://github.com/percy/percy-docs.git#69541cc83a6ddae7dc91cd097a6aa5c783c2987b",
    "prettier": "^1.8.1",
    "prettier-eslint-cli": "^4.4.0"
  },
  "engines": {
    "node": ">= 6.9.1"
  },
  "private": true
}
